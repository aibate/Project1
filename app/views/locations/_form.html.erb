<%= form_for @location do |f| %>
    <%= f.label :address %>
    <%= f.text_field :address, :required => true %>
    <%= f.label :harvest %>
    <%= f.text_field :harvest%>
    <%= f.label :image %>
    <%=  f.text_field :image %>
    <%= f.label :note %>
    <%= f.text_field :note %>

<%= f.submit %>
<% end %>